<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<VBox prefHeight="640.0" prefWidth="204.0" style="-fx-border-width: 0 1 0 0;" styleClass="border-container" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.components.navbar.AdminNavbarController">
    <children>
        <VBox spacing="8.0">
            <VBox.margin>
                <Insets />
            </VBox.margin>
            <children>
                <Button fx:id="displayAccountsButton" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" prefWidth="32.0" text="สรุปข้อมูลทั้งหมด" />
                <Button fx:id="manageLockerZonesButton" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" prefWidth="32.0" text="จัดการจุดบริการ" />
                <Button fx:id="manageOfficersButton" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" prefWidth="32.0" text="จัดการเจ้าหน้าที่" />
                <Button fx:id="manageUsersButton" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" prefWidth="32.0" text="จัดการผู้ใช้งาน" />
            </children>
        </VBox>
        <Region VBox.vgrow="ALWAYS" />
        <VBox fx:id="userTabVBox" spacing="16.0">
            <children>
            <Button fx:id="manualButton" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" prefWidth="32.0" text="คู่มือการใช้งาน" />
            <Rectangle arcHeight="2.0" arcWidth="2.0" fill="#d5d8dc" height="1.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" styleClass="fill-container" width="174.0" />
                <fx:include fx:id="miniUserProfile" source="../mini-profile-card.fxml" />
                <Button fx:id="logoutButton" maxWidth="1.7976931348623157E308" minHeight="32.0" mnemonicParsing="false" prefWidth="32.0" text="ออกจากระบบ" />
            </children>
        </VBox>
    </children>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="0.0" />
    </padding>
</VBox>
