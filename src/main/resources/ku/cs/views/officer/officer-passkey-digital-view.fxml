<DialogPane fx:id="passkeyDigitalDialogPane" maxHeight="392.0" minHeight="300.0" prefHeight="300.0" prefWidth="498.0" style="-fx-background-color: #FFFFFF;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.officer.DialogPane.PasskeyDigitalDialogPaneController">
