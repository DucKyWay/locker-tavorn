<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>


<?import java.lang.String?>
<AnchorPane maxHeight="150.0" maxWidth="300.0" minHeight="150.0" minWidth="300.0" prefHeight="150.0" prefWidth="300.0"
            xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ku.cs.controllers.admin.dialog.AdminEditZoneNameDialogController"
            fx:id="editZoneNameDialog">
    <VBox alignment="TOP_CENTER" prefHeight="80.0" prefWidth="300.0" spacing="4.0">
        <Region prefHeight="10.0" prefWidth="200.0"/>
        <Label text="จัดการจุดให้บริการ">
            <styleClass>
                <String fx:value="body-large" />
            </styleClass>
        </Label>
        <Region prefWidth="300" prefHeight="5.0"/>
        <Label text="กรอกชื่อจุดให้บริการใหม่">
            <styleClass>
                <String fx:value="label-medium" />
                <String fx:value="text-variant" />
            </styleClass>
        </Label>
        <Region prefWidth="300" prefHeight="5.0"/>
        <HBox alignment="TOP_CENTER" prefHeight="60.0" prefWidth="200.0">
            <Region prefWidth="5.0" prefHeight="100"/>
            <Label text="ชื่อจุดให้บริการ"/>
            <Region prefWidth="5.0" prefHeight="100"/>
            <TextField fx:id="zoneNameTextField"/>
            <Region prefWidth="5.0" prefHeight="100"/>
        </HBox>
    </VBox>
    <Button alignment="CENTER_RIGHT" layoutX="150.0" layoutY="100.0" mnemonicParsing="false" text="แก้ไขจุดให้บริการ" fx:id="submitButton">
        <styleClass>
            <String fx:value="filled-button" />
            <String fx:value="small" />
        </styleClass>
    </Button>
</AnchorPane>
